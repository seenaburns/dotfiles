  "tokenColors": [
    {
      "settings": {
        "foreground": "#CED8EA",
        "background": "#1D1C27",
        "bracketContentsOptions": "underline",
        "bracketContentsForeground": "#00FA8E",
        "bracketsOptions": "underline",
        "bracketsForeground": "#00FA8E",
        "tagsOptions": "underline",
        "tagsForeground": "#00FA8E",
        "findHighlight": "#314365",
        "findHighlightForeground": "#00FA8E",
        "gutter": "#1D1C27",
        "gutterForeground": "#636d8388",
        "selectionBorder": "#bbccf51b",
        "activeGuide": "#464c55",
        "stackGuide": "#464c55",
        "highlight": "#00FA8E",
        "highlightForeground": "#00FA8E",
        "shadow": "#1D1C27",
        "shadowWidth": "1"
      }
    },
    {
      "name": "Text",
      "scope": ["", "text", "source"],
      "settings": {
        "foreground": "#CED8EA",
        "fontStyle": ""
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#686B82",
        "fontStyle": ""
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword"],
      "settings": {
        "foreground": "#EDEFF2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#EDEFF2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "String",
      "scope": ["string"],
      "settings": {
        "foreground": "#00FA8E",
        "fontStyle": ""
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.other", "constant.character.escape"],
      "settings": {
        "foreground": "#EDEFF2",
        "fontStyle": ""
      }
    },
    {
      "name": "Function",
      "scope": ["support.function", "entity.name.function"],
      "settings": {
        "foreground": "#50bef7",
        "fontStyle": ""
      }
    },
    {
      "name": "Variable",
      "scope": ["variable"],
      "settings": {
        "foreground": "#50bef7",
        "fontStyle": ""
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#50bef7",
        "fontStyle": ""
      }
    },
    {
      "name": "Markdown header",
      "scope": ["heading"],
      "settings": {
        "foreground": "#EDEFF2",
        "fontStyle": ""
      }
    }
  ]
}
